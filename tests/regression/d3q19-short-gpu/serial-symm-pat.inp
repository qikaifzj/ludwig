##############################################################################
#
#  Symmetric patches initialisation smoke test
#
##############################################################################

##############################################################################
#
#  Run duration
#
###############################################################################

N_cycles 1

##############################################################################
#
#  System
#
##############################################################################

size 64_64_64
grid 4_4_1

##############################################################################
#
#  Fluid parameters
#
##############################################################################

viscosity 0.00625

##############################################################################
#
#  Free energy parameters
#
###############################################################################

free_energy symmetric

A -0.00625
B 0.00625
K 0.004
C 0.0

phi0 0.0
phi_initialisation  patches
phi_init_patch_size 2
phi_init_patch_vol  0.1

mobility 1.25

fd_gradient_calculation 3d_27pt_fluid
fd_advection_scheme_order 2

###############################################################################
#
#  Colloid parameters
#
###############################################################################

colloid_init        no_colloids

###############################################################################
#
#  Periodic conditions / boundaries
#
###############################################################################

boundary_walls_on no
periodicity 1_1_1

###############################################################################
#
#  Output frequency and type
#
###############################################################################

freq_statistics 1
config_at_end no

###############################################################################
#
#  Miscellaneous
#
#  random_seed  +ve integer is the random number generator seed
#
###############################################################################

random_seed 8361235
